{
  "ElMessage.info": {
    "prefix": "info",
    "body": ["ElMessage.info({", "  message: '$1',", "  showClose: true,", "});"],
    "description": "普通类型的消息提示 (ElMessage)"
  },
  "ElMessage.success": {
    "prefix": "success",
    "body": ["ElMessage.success({", "  message: '$1',", "  showClose: true,", "});"],
    "description": "成功类型的消息提示 (ElMessage)"
  },
  "ElMessage.warning": {
    "prefix": "warning",
    "body": ["ElMessage.warning({", "  message: '$1',", "  showClose: true,", "});"],
    "description": "警告类型的消息提示 (ElMessage)"
  },
  "ElMessage.error": {
    "prefix": "error",
    "body": ["ElMessage.error({", "  message: '$1',", "  showClose: true,", "});"],
    "description": "错误类型的消息提示 (ElMessage)"
  },

  "ElMessageBox.alert": {
    "prefix": "alert",
    "body": [
      "ElMessageBox.alert('内容', '标题', {",
      "  confirmButtonText: '确定',",
      "  callback: action => {",
      "    if (action === 'confirm') {",
      "      $1",
      "    }",
      "  }",
      "});"
    ],
    "description": "alert消息弹出框 (ElMessageBox)"
  },
  "ElMessageBox.confirm": {
    "prefix": "confirm",
    "body": [
      "ElMessageBox.confirm('内容', '标题', {",
      "  {",
      "     confirmButtonText: '确定'",
      "     cancelButtonText: '取消'",
      "  }",
      "}).then(() => {",
      "  $1",
      "}).catch(() => {",
      "  ",
      "});"
    ],
    "description": "confirm消息弹出框 (ElMessageBox)"
  },
  "ElMessageBox.prompt": {
    "prefix": "prompt",
    "body": [
      "ElMessageBox.prompt('内容', '标题', {",
      "  {",
      "     confirmButtonText: '确定'",
      "     cancelButtonText: '取消'",
      "  }",
      "}).then(() => {",
      "  $1",
      "}).catch(() => {",
      "  ",
      "});"
    ],
    "description": "prompt消息弹出框 (ElMessageBox)"
  },
  "ElNotification.info": {
    "prefix": "info",
    "body": ["ElNotification.info({", "  title: '温馨提示',", "  message: '$1',", "});"],
    "description": "普通类型的消息通知 (ElNotification)"
  },
  "ElNotification.success": {
    "prefix": "success",
    "body": ["ElNotification.success({", "  title: '成功',", "  message: '$1',", "});"],
    "description": "成功类型的消息通知 (ElNotification)"
  },
  "ElNotification.warning": {
    "prefix": "warning",
    "body": ["ElNotification.warning({", "  title: '警告',", "  message: '$1',", "});"],
    "description": "警告类型的消息通知 (ElNotification)"
  },
  "ElNotification.error": {
    "prefix": "error",
    "body": ["ElNotification.error({", "  title: '错误',", "  message: '$1',", "});"],
    "description": "错误类型的消息通知 (ElNotification)"
  }
}
